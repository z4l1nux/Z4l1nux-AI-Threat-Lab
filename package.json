{
  "name": "threat-modeling-co-pilot-with-ai",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "check-prerequisites": "node scripts/check-prerequisites.js",
    "setup": "node scripts/setup.js",
    "prestart": "node scripts/setup.js",
    "predev:full": "node scripts/setup.js",
    "dev": "vite",
    "dev:backend": "cd backend && ts-node --project tsconfig.json src/server.ts",
    "dev:full": "concurrently --kill-others --names \"BACKEND,FRONTEND\" --prefix-colors \"bgBlue.bold,bgMagenta.bold\" \"npm run dev:backend\" \"npm run dev\"",
    "build": "npm run build:backend && npm run build:frontend",
    "build:backend": "cd backend && tsc",
    "build:frontend": "vite build",
    "start": "concurrently --kill-others --names \"BACKEND,FRONTEND\" --prefix-colors \"bgBlue.bold,bgMagenta.bold\" \"npm run start:backend\" \"npm run preview\"",
    "start:backend": "cd backend && node dist/server.js",
    "preview": "vite preview",
    "create-neo4j": "cd backend && ts-node --project tsconfig.json src/scripts/initNeo4j.ts",
    "test-rag": "cd backend && ts-node --project tsconfig.json src/scripts/testRAG.ts",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:integration": "./test-rag.sh",
    "postinstall": "cd backend && npm install"
  },
  "dependencies": {
    "@google/genai": "1.21.0",
    "@langchain/community": "0.3.57",
    "@langchain/google-genai": "0.1.12",
    "concurrently": "9.2.1",
    "cors": "2.8.5",
    "csv-parser": "3.2.0",
    "dotenv": "16.6.1",
    "express": "4.21.2",
    "html2canvas": "1.4.1",
    "jspdf": "3.0.3",
    "jspdf-autotable": "5.0.2",
    "langchain": "0.3.35",
    "multer": "2.0.2",
    "neo4j-driver": "5.28.2",
    "node-fetch": "2.7.0",
    "pdf-parse": "1.1.1",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "react-markdown": "10.1.0",
    "reactflow": "^11.11.4",
    "remark-gfm": "4.0.1",
    "xml2js": "0.6.2"
  },
  "devDependencies": {
    "@testing-library/dom": "10.4.1",
    "@testing-library/jest-dom": "6.9.0",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/cors": "2.8.19",
    "@types/express": "5.0.3",
    "@types/multer": "1.4.13",
    "@types/node": "22.18.7",
    "@types/node-fetch": "2.6.13",
    "@types/react": "19.1.16",
    "@types/react-dom": "19.1.9",
    "@types/xml2js": "0.4.14",
    "@vitejs/plugin-react": "4.7.0",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "autoprefixer": "10.4.21",
    "jsdom": "25.0.1",
    "postcss": "8.5.6",
    "tailwindcss": "4.1.13",
    "ts-node": "10.9.2",
    "typescript": "5.7.3",
    "vite": "6.3.6",
    "vitest": "3.2.4"
  }
}
